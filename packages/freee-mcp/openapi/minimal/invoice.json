{
  "paths": {
    "/delivery_slips": {
      "get": {
        "summary": "納品書一覧の取得\n",
        "description": "納品書の一覧を返します。",
        "parameters": [
          {
            "name": "company_id",
            "in": "query",
            "type": "integer",
            "required": true,
            "description": "事業所ID"
          },
          {
            "name": "delivery_slip_number",
            "in": "query",
            "type": "string",
            "required": false,
            "description": "納品書番号"
          },
          {
            "name": "subject",
            "in": "query",
            "type": "string",
            "required": false,
            "description": "件名"
          },
          {
            "name": "partner_ids",
            "in": "query",
            "type": "string",
            "required": false,
            "description": "取引先ID（半角数字のidを半角カンマ区切りスペースなしで指定してください。最大3件まで指定できます。）"
          },
          {
            "name": "payment_status",
            "in": "query",
            "type": "string",
            "required": false,
            "description": "入金ステータス（unsettled: 入金待ち, settled: 入金済み）"
          },
          {
            "name": "deal_status",
            "in": "query",
            "type": "string",
            "required": false,
            "description": "取引ステータス（registered: 登録済み、 unregistered: 登録待ち）"
          },
          {
            "name": "sending_status",
            "in": "query",
            "type": "string",
            "required": false,
            "description": "送付ステータス（sent: 送付済み、 unsent: 送付待ち）"
          },
          {
            "name": "cancel_status",
            "in": "query",
            "type": "string",
            "required": false,
            "description": "取消済み（canceled: 該当する、 uncanceled: 該当しない）"
          },
          {
            "name": "start_delivery_slip_date",
            "in": "query",
            "type": "string",
            "required": false,
            "description": "納品日の開始日"
          },
          {
            "name": "end_delivery_slip_date",
            "in": "query",
            "type": "string",
            "required": false,
            "description": "納品日の終了日"
          },
          {
            "name": "limit",
            "in": "query",
            "type": "integer",
            "required": false,
            "description": "取得レコードの件数 (デフォルト: 20, 最小: 1, 最大: 100)"
          },
          {
            "name": "offset",
            "in": "query",
            "type": "integer",
            "required": false,
            "description": "取得レコードのオフセット (デフォルト: 0)"
          }
        ]
      },
      "post": {
        "summary": "納品書の作成\n",
        "description": "納品書の作成をします。\n\nissue_date, account_item_id, tax_code, item_id, section_id, tag_ids, segment_1_tag_id, segment_2_tag_id, segment_3_tag_id は、取引登録の下書き保存で利用されます。\n\ntag_idsは10個まで設定可能です。",
        "hasJsonBody": true
      }
    },
    "/delivery_slips/templates": {
      "get": {
        "summary": "使用可能な納品書の帳票テンプレート一覧の取得\n",
        "description": "使用可能な納品書の帳票テンプレート一覧を返します。",
        "parameters": [
          {
            "name": "company_id",
            "in": "query",
            "type": "integer",
            "required": true,
            "description": "事業所ID"
          }
        ]
      }
    },
    "/delivery_slips/{id}": {
      "get": {
        "summary": "納品書の取得\n",
        "description": "指定されたIDの納品書を返します。",
        "parameters": [
          {
            "name": "company_id",
            "in": "query",
            "type": "integer",
            "required": true,
            "description": "事業所ID"
          },
          {
            "name": "id",
            "in": "path",
            "type": "integer",
            "required": true,
            "description": "納品書ID"
          }
        ]
      },
      "put": {
        "summary": "納品書の更新\n",
        "description": "納品書の更新をします。\n\nissue_date, account_item_id, tax_code, item_id, section_id, tag_ids, segment_1_tag_id, segment_2_tag_id, segment_3_tag_id は、取引登録の下書き保存で利用されます。\n\ntag_idsは10個まで設定可能です。",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "type": "integer",
            "required": true,
            "description": "納品書ID"
          }
        ],
        "hasJsonBody": true
      }
    },
