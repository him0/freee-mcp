{
  "type": "module",
  "name": "@him0/freee-mcp",
  "version": "0.6.0",
  "main": "dist/index.cjs",
  "module": "dist/index.esm.js",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs"
    }
  },
  "bin": {
    "freee-mcp": "./bin/cli.js"
  },
  "scripts": {
    "build": "pnpm run build:types && pnpm run build:esbuild",
    "build:esbuild": "tsx build.ts",
    "build:types": "tsc --declaration --emitDeclarationOnly --declarationDir './dist'",
    "start": "tsx src/index.ts",
    "dev": "tsx watch src/index.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint --fix 'src/**/*.ts'",
    "knip": "knip",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "inspector": "mcp-inspector pnpm run start",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "generate:references": "tsx scripts/generate-references.ts",
    "fetch:schemas": "tsx scripts/fetch-schemas.ts"
  },
  "author": "him0",
  "license": "ISC",
  "description": "Model Context Protocol (MCP) server for freee API integration",
  "devDependencies": {
    "@modelcontextprotocol/inspector": "^0.17.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "jsdom": "^26.1.0",
    "knip": "^5.78.0",
    "yaml": "^2.8.2"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.24.3",
    "open": "^10.2.0",
    "prompts": "^2.4.2",
    "zod": "^3.25.76"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "freee",
    "api",
    "accounting",
    "claude"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/him0/freee-mcp.git",
    "directory": "packages/freee-mcp"
  },
  "files": [
    "dist/",
    "bin/",
    "README.md"
  ]
}
